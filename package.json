{
  "name": "@nuxtjs/svg-sprite",
  "version": "1.0.0-beta.3",
  "description": "Easy way to use and optimize svg files in Nuxt.js",
  "repository": "nuxt-community/svg-sprite-module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxt dev playground",
    "dev:docs": "nuxi dev docs",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .ts --ext .vue .",
    "prepublishOnly": "npm run build",
    "release": "npm test && standard-version && git push --follow-tags && npm publish",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "3.2.0",
    "svgo": "^2.8.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/test-utils": "3.2.0",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/svgo": "^2.6.3",
    "eslint": "^8.19.0",
    "nuxt": "3.2.0",
    "playwright": "^1.23.3",
    "standard-version": "latest",
    "vitest": "^0.28.4",
    "vue": "3.2.47"
  }
}
